<?xml version="1.0" encoding="UTF-8" ?>
<widget id="com.mycompany.shresh" version="1.0.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
	<name>TheVikingConqueror</name>
	<description>The Viking Conqueror</description>
	<author></author>

	<icon src="www/icon-256.png" width="256" height="256" density="xxxhdpi" />
